<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
  <NameField>$HN2DevShoeSpiderName</NameField>

  <DialogueNode>
    <Name>Start</Name>
    <EntryCondition>DEFAULT</EntryCondition>
    <Dialogue>
      <Page>$HN2DevShoeHub</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>$HN2DevJamMessage</Text>
        <DialogueTarget>Jam</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>$HN2DevVictoryMessage</Text>
        <DialogueTarget>Victory</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>$HN2DevGoodbye</Text>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>Jam</Name>
    <Dialogue>
      <Page>$HN2DevShoe1</Page>
      <Page>$HN2DevShoe2</Page>
      <Page>$HN2DevShoe3</Page>
    </Dialogue>
    <DialogueTarget>Start</DialogueTarget>
  </DialogueNode>

  <DialogueNode>
    <Name>Victory</Name>
    <Dialogue>
      <Page>$HN2DevShoeV1</Page>
      <Page>$HN2DevShoeV2</Page>
    </Dialogue>
    <DialogueTarget>Start</DialogueTarget>
  </DialogueNode>

</DialogueTree>