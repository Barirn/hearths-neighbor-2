<DialogueTree xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/Outer-Wilds-New-Horizons/new-horizons/main/NewHorizons/Schemas/dialogue_schema.xsd">
  <NameField>$HN2DevGameWyrmName</NameField>

  <DialogueNode>
    <Name>Start</Name>
    <EntryCondition>DEFAULT</EntryCondition>
    <Dialogue>
      <Page>$HN2DevGameWyrmHub</Page>
    </Dialogue>
    <DialogueOptionsList>
      <DialogueOption>
        <Text>$HN2DevJamMessage</Text>
        <DialogueTarget>Jam</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>$HN2DevVictoryMessage</Text>
        <DialogueTarget>Victory</DialogueTarget>
      </DialogueOption>
      <DialogueOption>
        <Text>$HN2DevGoodbye</Text>
      </DialogueOption>
    </DialogueOptionsList>
  </DialogueNode>

  <DialogueNode>
    <Name>Jam</Name>
    <Dialogue>
      <Page>$HN2DevGameWyrm1</Page>
      <Page>$HN2DevGameWyrm2</Page>
      <Page>$HN2DevGameWyrm3</Page>
      <Page>$HN2DevGameWyrm4</Page>
      <Page>$HN2DevGameWyrm5</Page>
    </Dialogue>
    <DialogueTarget>Start</DialogueTarget>
  </DialogueNode>

  <DialogueNode>
    <Name>Victory</Name>
    <Dialogue>
      <Page>$HN2DevGameWyrmV1</Page>
      <Page>$HN2DevGameWyrmV2</Page>
      <Page>$HN2DevGameWyrmV3</Page>
      <Page>$HN2DevGameWyrmV4</Page>
    </Dialogue>
    <DialogueTarget>Start</DialogueTarget>
  </DialogueNode>

</DialogueTree>